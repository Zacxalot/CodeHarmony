{
  "allow_websocket_upgrade": true,
  "root": "build/",
  "routes": {
    "/**": "index.html"
  },
  "proxies": {
    "/plan/": {
      "origin": "${API_URL}/plan/"
    },
    "/session/": {
      "origin": "${API_URL}/session/"
    },
    "/account/": {
      "origin": "${API_URL}/account/"
    },
    "/ws/": {
      "origin": "${API_URL}/ws/"
    },
    "/run/": {
      "origin": "${API_URL}/run/"
    }
  }
}